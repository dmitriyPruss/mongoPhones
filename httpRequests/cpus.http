# POST - create a phone with a certain CPU #
POST http://localhost:5000/api/cpus/2/phones HTTP/1.1
Content-Type: application/json

{
  "model": "Xiaomi",
  "brand": "Note X",
  "manufacturedYear": 2021,
  "RAMsize": 8,
  "screenDiagonal": 5.6,
  "isNFC": true
}

